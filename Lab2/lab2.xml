int listB[N+1];

void enqueue(int id, bool isModeA){
  if(isModeA)
	listA[lenA++] = id;
  else
       	listB[lenB++] = id;
}

int front(){
  if(modeA)
	return listA[0];
  else
       return listB[0];
}

int getLength(){
  if(modeA)
	return lenA;
  else
       return lenB;
}

void dequeue(){
 int i = 0;
 if(modeA){
	lenA-=1;
	while (i &lt; lenA)
 	{
 		listA[i] = listA[i + 1];
 		i++;
 	}
 	listA[i] = 0;
  }
  else{
	lenB-=1;
	while (i &lt; lenB)
 	{
 		listB[i] = listB[i + 1];
 		i++;
 	}
 	listB[i] = 0;
  }
}

void insertFirst(int id, bool isModeA){
  int i;
  if(isModeA){
	i = lenA;
	lenA+=1;
	while (i &gt;= 0)
 	{
 		listA[i+1] = listA[i];
 		i--;
	}
 	listA[0] = id;
  }
  else{
	i = lenB;
	lenB+=1;
	while (i &gt;= 0)
 	{
 		listB[i+1] = listB[i];
 		i--;
 	}
 	listB[0] = id;
  }
